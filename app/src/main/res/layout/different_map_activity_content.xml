<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.gaodemapdemo.gaode.DifferentMapSDKActivity"
    tools:showIn="@layout/activity_different_map">


    <com.amap.api.maps.MapView
        android:id="@+id/mv_map"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    </com.amap.api.maps.MapView>

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/acb_btn_tian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="去天地图" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/acb_btn"
        android:layout_width="wrap_content"
        android:text="去谷歌"
        android:layout_marginLeft="100dp"
        android:layout_height="wrap_content" />

    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/tv_zoom_level"
        android:layout_width="wrap_content"
        android:text="缩放等级"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="200dp"
        android:layout_height="wrap_content" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/amap_with_src_tian_di_tu_btn"
        android:layout_width="wrap_content"
        android:text="天地图数据源"
        android:layout_toEndOf="@id/tv_zoom_level"
        android:layout_height="wrap_content" />

</RelativeLayout>
